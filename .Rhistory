#' Creates a random DNA
#' @export
dnar <- function(n) {
chr <- paste0('GTC', sample(names(Biostrings::GENETIC_CODE), (n - 1) %/% 3, T), collapse = '')
dna <- Biostrings::DNAString(chr)
return(dna)
}
